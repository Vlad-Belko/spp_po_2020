<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="281.0" prefWidth="219.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ComputerCompany.Presentation.Controllers.CreateAddressController">
   <children>
       <VBox layoutX="10.0" layoutY="19.0" prefHeight="261.0" prefWidth="199.0" spacing="5.0"
             AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
             AnchorPane.topAnchor="20.0">
           <padding>
               <Insets top="10.0"/>
           </padding>
           <Label text="Country"/>
           <TextField fx:id="country"/>
           <Label text="City"/>
           <TextField fx:id="city"/>
           <Label text="Street"/>
           <TextField fx:id="street"/>
           <Label text="Postal code"/>
           <TextField fx:id="postalCode"/>
           <Button onAction="#OnCreateButtonClicked" mnemonicParsing="false" text="Create"/>
       </VBox>
      <Label layoutX="10.0" layoutY="5.0" text="Create Address">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font></Label>
   </children>
</AnchorPane>
